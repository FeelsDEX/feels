    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.18.1",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "https-browserify": "^1.0.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.101.3",
    "ws": "^8.18.3"
  }
}
